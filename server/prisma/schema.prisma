generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  name        String
  email       String
  password    String
  ip          String
  expireToken Json[]
  createDate  DateTime @default(now())
  tasks       Task[]
}

model Task {
  id         String   @id @default(cuid())
  createDate DateTime @default(now())
  task       String
  priority   Int
  status     String   @default("Pending")
  userId     String
  user       User     @relation(fields: [userId], references: [id])
}
